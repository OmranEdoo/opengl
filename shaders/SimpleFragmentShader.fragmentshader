#version 330 core

out vec4 color;

in vec3 fragmentColor;

in vec2 UV;

uniform sampler2D myTextureSampler;

void main(){
    color = vec4((texture(myTextureSampler,UV).rgb[0]+fragmentColor[0])/2, (texture(myTextureSampler,UV).rgb[1]+fragmentColor[1])/2, (texture(myTextureSampler,UV).rgb[2]+fragmentColor[2])/2, 1.0);;
    //colorvs = vec4(fragmentColor, 1.0);
}
